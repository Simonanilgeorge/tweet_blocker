{
    "manifest_version": 3,
    "name": "Tweet Blocker",
    "version": "1.0",
    "description": "Block tweets containing specified keywords for a customizable Twitter experience.",
    "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    },
    "action": {
      "default_icon": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
      }
    },
    "content_scripts": [
      {
        "js": [
          "content.js"
        ],
        "css":["style.css"],
        "matches": [
          "https://twitter.com/*"
        ]
      }
    ]
  }
  